{
    "name": "Developer Browser History",
    "version": "0.0.8",
    "manifest_version": 2,
    "description": "Developer Browser History",
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "icon-16.png"
    },
    "permissions": [
        "tabs",
        "history"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
